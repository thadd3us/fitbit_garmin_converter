{
  "name": "My Dev Container",
  "build": {
    "dockerfile": "../Dockerfile",
    "context": ".."
  },
  "mounts": [
    "source=${localWorkspaceFolder},target=/workspace,type=volume"
  ],
  // "workspaceMount": "source=thad-and-andrew-volume,target=/workspace,type=volume",
  "workspaceFolder": "/workspace",
  "postCreateCommand": "uv sync --all-packages --active",
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python"
      ]
    }
  }
}